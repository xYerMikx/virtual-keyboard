{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["airbnb-base"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "import/prefer-default-export": "off", //you cannot import a class with default package.
        "import/extensions": "off" // app doesn't see import without extensions.
    }
}
