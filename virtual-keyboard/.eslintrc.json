{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["airbnb-base"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-underscore-dangle": "off",
        "no-unused-vars": "off",
        "semi": "off",
        "no-trailing-spaces": "off",
        "padded-blocks": "off",
        "import/prefer-default-export": "off",
        "import/extensions": "never"
    }
}
